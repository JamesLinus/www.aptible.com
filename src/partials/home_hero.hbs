  <div class="product-stack--feature feature-compliance">
    <h2 class="feature-title">{{product_stack.compliance.title}}</h2>
    <div class="feature-body">
      {{product_stack.compliance.body }}
      <a class="btn btn-lg btn-primary" href="{{product_stack.compliance.cta.href}}">{{product_stack.compliance.cta.text}}</a>
    </div>
  </div>
</div>
