<h1 class="product-stack--title primary-title">{{hero_title}}</h1>
<div class="product-stack">
  <div class="product-stack--feature feature-deployment">
    <h2 class="feature-title">{{product_stack.deployment.title}}</h2>
    <div class="feature-body">
      {{product_stack.deployment.body }}
      <a class="btn btn-lg btn-primary" href="{{product_stack.deployment.cta.href}}">{{product_stack.deployment.cta.text}}</a>
    </div>
  </div>
  <div class="product-stack--deployment-guide">
    <svg width="62px" height="250px" viewBox="0 0 62 250">
        <g id="deployment-guide" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
          <g transform="translate(0.000000, 1.000000)" stroke="#3498DB" stroke-width="2">
